<!DOCTYPE html>
<html>
  <head>
    <style type="text/css">
      html, body { height: 100%; margin: 0; padding: 0; }
      #map { height: 100%; }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script type="text/javascript">

      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 14.048093, lng: -87.173986 },
          //center: {lat: position.coords.latitude, lng: position.coords.longitude},
          zoom: 16
        });
        
        /*navigator.geolocation.getCurrentPosition(function(position) {
          
          
          
        });*/
        
      }

    </script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDde72KW2j3dLlBjRYH-YTcw_FIfJZYiVQ&&libraries=geometry&callback=initMap">
    </script>
  </body>
</html>